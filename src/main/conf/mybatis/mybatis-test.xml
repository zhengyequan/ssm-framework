<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="test">
	<resultMap type="com.yqbaba.test.entity.Test" id="Test"/>
	
	<select id="getUserByIdCard" resultMap="Test">
		select * from Test where idCard=#{idCard}
	</select>
	
	<insert id="createTest">
		insert into Test(
			idCard, cardNo
		) values (
			#{idCard}, #{cardNo}
		)
	</insert>
</mapper>
